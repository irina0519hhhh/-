#include <stdio.h>

/* Пусть в программе определены две переменных:
int a = 10;
int b = 4;
Напишите программу, в которой определите третью переменную, которая равна результату деления перевой переменной на вторую.*/


int a = 10, b = 4;
double del;
int main()
{ del = a/b;
    printf("\'a', \'b' devision result is: %lf", del);
    return 0;
}


