#include <stdio.h>
#include <math.h>
double a = 5, b = 6, c = 1, x1, x2, x, d;
int
main ()
{
  d = b * b - 4 * a * c;
  x1 = (-b + sqrt (d)) / (2 * a);
  x2 = (-b - sqrt (d)) / (2 * a);
  if (d == 0)
	{
	  x = (-b + sqrt (d)) / (2 * a);
	  printf ("x = %lf", x);
	}
  if (d > 0)
	{
	  printf ("d = %lf\nx1 = %lf\nx2 = %lf", d, x1, x2);
	}
  if (d < 0)
	{
	  printf ("there're no roots!");
	}
  return 0;
}
